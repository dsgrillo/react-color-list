{
  "name": "react-color-list",
  "version": "1.0.8",
  "description": "A color list manager for React",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "build": "rm -rf dist && mkdir dist &&  babel src/. -d dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthewgferrari/react-color-list.git"
  },
  "keywords": [
